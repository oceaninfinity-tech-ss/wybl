{
  "name": "gui",
  "type": "module",
  "scripts": {
    "build": "npm run build:bundle && npm run build:dts",
    "build:bundle": "node build.js",
    "build:dts": "tsc && dts-bundle-generator --no-banner --inline-declare-externals --no-check dist/types/exported.d.ts -o dist/guis.d.ts"
  },
  "description": "SSS GUI",
  "devDependencies": {
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.25.11",
    "esbuild-sass-plugin": "^3.3.1",
    "sass": "^1.90.0",
    "typescript": "^5.9.2"
  }
}
